function binary(f, a, b) {
	f(a, b);
}

var add = function(a, b) {
	a + b;
};

binary(add, 3, 4);
binary(function(a, b) { a * b; }, 3, 7);

var div = function(a, b) {
	if b == 0 {
		0;
	} else if b == 1 {
		a;
	} else {
		a / b;
	}
};

div(5, 0);
div(5, 1);
div(5, 2);

var i, sum = 0;
for i = 0; i < 10; i = i + 1 {
	sum = sum + i;
}

for sum != 0 {
	sum = sum - 1;
}
